{
	"name": "@ctx-core/pocketgrid",
	"version": "6.0.9",
	"description": "ctx-core theme support",
	"keywords": [
		"ctx-core",
		"time"
	],
	"homepage": "https://github.com/ctx-core/theme--pocketgrid#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/theme--pocketgrid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/theme--pocketgrid/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@ctx-core/sass": "workspace:^12.1.7",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"svelte": "3.44.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
